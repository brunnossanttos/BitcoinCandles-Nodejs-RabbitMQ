{
    "compilerOptions": {
        "lib": [
            "es5",
            "es6"
        ],
        "baseUrl": "./",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "./build",
        "rootDir": "./src",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true
    }
}
